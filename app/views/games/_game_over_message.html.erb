<% if game && game.player_1 && game.player_2 %>
  <% winner = game.winner %>
  <% if winner %>
    <%
      name =
        if winner == game.player_1
          "Player 1"
        elsif winner == game.player_2
          "Player 2"
        else
          nil
        end
      %>

    <%= "Game Over: #{name} wins" %>
  <% end %>
<% end %>
